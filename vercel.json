{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://test-prod.runasp.net/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://tccd-cufe.me"
        }
      ]
    }
  ]
}